/usuario/obtenerPorNombre

entrada : NombreUsuario

salida:

nIdUsuario;
bActivo;
bBloqueado;
dFechaCreacion;
nIntentos;
String sContrasenia;
sEmail;
sNombreUsuario;


/usuario/obtenerPermisos

entrada : IdUsuario


salida: Lista de :

nIdPermiso;
bActivo;
bEscritura;
bLectura;
dDesde;
dHasta;


/usuario/obtenerPreguntasSeguridad

